<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/q2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2E2E2E"
    tools:context=".test2">

    <ImageView
        android:id="@+id/char2"
        android:layout_width="0dp"
        android:layout_height="659dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/downbutton2"
        app:layout_constraintEnd_toEndOf="@+id/upbutton2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/characterresult" />

    <Button
        android:id="@+id/downbutton2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="45dp"
        android:backgroundTint="#DF01A5"
        android:text="나는 인생에서 자극적인 것을 찾아다니는 편이다."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/upbutton2" />

    <TextView
        android:id="@+id/q1"
        android:layout_width="125dp"
        android:layout_height="52dp"
        android:layout_marginBottom="421dp"
        android:fontFamily="@font/ddangs"
        android:text="질문2"
        android:textColor="#FFFFFF"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/downbutton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/upbutton2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="472dp"
        android:layout_marginEnd="34dp"
        android:backgroundTint="#DF01A5"
        android:text="나는 모든 상황을 진지하게 받아들이는 편이다"
        app:layout_constraintBottom_toTopOf="@+id/downbutton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="94dp"
        android:layout_height="110dp"
        android:layout_marginStart="64dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/upbutton2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/question" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="81dp"
        android:layout_height="98dp"
        android:layout_marginStart="284dp"
        android:layout_marginBottom="247dp"
        app:layout_constraintBottom_toBottomOf="@+id/char2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/question" />

</androidx.constraintlayout.widget.ConstraintLayout>
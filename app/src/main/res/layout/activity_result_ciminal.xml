<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2E2E2E"
    android:fontFamily="@font/ddangs"
    android:orientation="vertical"
    tools:context=".Result_criminal">

    <TextView
        android:id="@+id/annaactor2"
        android:layout_width="198dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:text="출연 : 김상중, 강예원, 민지아, 박해진 외"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toBottomOf="@+id/annaage2" />

    <TextView
        android:id="@+id/annaage2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="청소년 관람 불가"
        android:textColor="#a0a0a0"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toBottomOf="@+id/annatitle2" />

    <TextView
        android:id="@+id/annaott2"
        android:layout_width="214dp"
        android:layout_height="51dp"
        android:layout_marginTop="10dp"
        android:text="시청 가능 OTT : 디즈니 플러스, 티빙"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toBottomOf="@+id/annaactor2" />

    <TextView
        android:id="@+id/annatitle2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="260dp"
        android:text="나쁜 녀석들(2014)"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView9" />

    <TextView
        android:id="@+id/textanimation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/ddangs"
        android:text="당신의 취향은 &quot;범죄물&quot; 입니다"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="29sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="200dp"
        android:layout_height="200sp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.004"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textanimation"
        app:srcCompat="@drawable/annabelle" />

    <TextView
        android:id="@+id/octitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="오션스8(2018)"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textanimation" />

    <TextView
        android:id="@+id/annaage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="15세 이용가"
        android:textColor="#a0a0a0"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/octitle" />

    <TextView
        android:id="@+id/annaactor"
        android:layout_width="180dp"
        android:layout_height="47dp"
        android:layout_marginTop="10dp"
        android:text="출연 : 애나벨 월리스, 워드 호튼, 알프리 우다드 외"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/annaage" />

    <TextView
        android:id="@+id/annaott"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="시청 가능 OTT : 넷플릭스"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/annaactor" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/annaott"
        app:srcCompat="@drawable/netfilx" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="200sp"
        android:layout_height="200sp"
        android:layout_marginBottom="90dp"
        android:src="@drawable/badguys"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>